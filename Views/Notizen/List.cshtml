@model IEnumerable<Wörter.AppData.Entities.Notiz>?

<partial name="_Menu" />

<hr>

<table class="table table-striped">
	<thead>
		<tr>
			<th>#</th>
			<th>Titles</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var i in Model ?? Enumerable.Empty<Wörter.AppData.Entities.Notiz>())
		{
			<tr>
				<th>
					<a href="/CKE/@i.Id" class="text-decoration-none text-success"><i class="fa-solid fa-pen-to-square me-4"></i></a>
					<a href="/CKEDelete/@i.Id" class="text-decoration-none text-danger"><i class="fa-solid fa-trash"></i></a>
				</th>
				<td>
					<a href="/CKEView/@i.Id" class="text-decoration-none">@i.Title</a>
				</td>
			</tr>
		}
	</tbody>
</table>

